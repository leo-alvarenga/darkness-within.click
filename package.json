{
  "name": "darkness-within.click",
  "version": "0.12.0",
  "homepage": "https://darkness-within.click",
  "type": "module",
  "scripts": {
    "clear": "rm -rf ./build && rm -rf ./dist && rm -rf ./node_modules && mkdir dist",
    "dev": "tailwindcss -w & vite",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}' --config ./.prettierrc",
    "build": "tailwindcss -o build.css --minify && mv build.css ./dist && tsc && vite build && mv dist/* ./build",
    "preview": "vite preview",
    "predeploy": "npm run clear && npm install && npm run build"
  },
  "dependencies": {
    "dayjs": "^1.11.7",
    "i18next": "^22.4.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.21",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  }
}
